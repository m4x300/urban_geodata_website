---
pagination:
    data: maps
    size: 1
    alias: map
permalink: "/{{ map.slug }}/index.html"
layout: "page.njk"
eleventyComputed:
    title: "{{ map.title }}"
---
<main class="descr-page">
<img src="/{{ map.preview }}" class="descr-preview" alt="Ausschnitt der historischen Karte '{{ map.title }}'" />
<h1>{{ map.title }}</h1>
<p><a href="/{{ map.slug }}/map/">Karte öffnen</a></p>
{% include "descriptions/" + (map.slug) + ".html" ignore missing %}
<nav>
    <a href="/{{ map.slug }}/map/">Karte öffnen</a><br>
    <a href="/">Zurück zur Übersicht</a>
</nav>
</main>
